<extension xmlns="urn:newrelic-extension">
  <instrumentation>
    <tracerFactory>
      <match assemblyName="StackExchange.Redis" className="StackExchange.Redis.ConnectionMultiplexer">
        <exactMethodMatcher methodName="ExecuteSyncImpl" />
        <exactMethodMatcher methodName="ExecuteAsyncImpl" />
      </match>
      <match assemblyName="StackExchange.Redis.StrongName" className="StackExchange.Redis.ConnectionMultiplexer">
        <exactMethodMatcher methodName="ExecuteSyncImpl" />
        <exactMethodMatcher methodName="ExecuteAsyncImpl" />
      </match>
    </tracerFactory>
    <tracerFactory name="stackexchangeredis-connect">
      <match assemblyName="StackExchange.Redis" className="StackExchange.Redis.Configuration.DefaultOptionsProvider">
        <exactMethodMatcher methodName="AfterConnectAsync" />
      </match>
    </tracerFactory>
  </instrumentation>
</extension>
